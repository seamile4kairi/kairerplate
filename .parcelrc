{
  "extends": [
    "@parcel/config-default"
  ],
  "transformers": {
    "*.ejs": [
      "parcel-transformer-ejs"
    ]
  },
  "resolvers": [
    "parcel-resolver-exclude-assets",
    "@parcel/resolver-default"
  ],
  "namers": [
    "parcel-namer-rewrite"
  ],
  "reporters": [
    "...",
    "parcel-reporter-static-files-copy"
  ]
}
